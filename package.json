{
  "name": "taro-weapp",
  "version": "1.0.0",
  "private": true,
  "description": "taro-weapp",
  "templateInfo": {
    "name": "taro-hooks",
    "typescript": true,
    "css": "less"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write",
      "eslint --fix"
    ]
  },
  "scripts": {
    "prepare": "husky install",
    "mock": "mock-server -p 8001",
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "test": "jest",
    "ci": "jest --bail --silent",
    "update:deps": "yarn upgrade-interactive --latest"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
  "dependencies": {
    "@antv/f2": "^3.8.10-beta.1",
    "@babel/runtime": "^7.17.2",
    "@taroify/core": "^0.0.27-alpha.0",
    "@tarojs/cli": "3.4.1",
    "@tarojs/components": "3.4.1",
    "@tarojs/extend": "^3.4.1",
    "@tarojs/plugin-framework-react": "3.4.1",
    "@tarojs/plugin-html": "^3.4.1",
    "@tarojs/react": "3.4.1",
    "@tarojs/runtime": "3.4.1",
    "@tarojs/taro": "3.4.1",
    "dva-core": "^2.0.4",
    "dva-immer": "^1.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "taro-hooks": "1.5.3"
  },
  "devDependencies": {
    "@babel/core": "^7.17.4",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@liangskyli/mock": "^0.9.0",
    "@tarojs/mini-runner": "3.4.1",
    "@tarojs/webpack-runner": "3.4.1",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/react": "^17.0.39",
    "@types/webpack-env": "^1.16.3",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "babel-plugin-import": "^1.13.3",
    "babel-preset-taro": "3.4.1",
    "eslint": "^8.9.0",
    "eslint-config-taro": "3.4.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "stylelint": "^14.5.1",
    "stylelint-config-css-modules": "^3.0.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.5.0",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
