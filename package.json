{
  "name": "taro-weapp",
  "version": "1.0.0",
  "private": true,
  "description": "taro-weapp",
  "templateInfo": {
    "name": "taro-hooks",
    "typescript": true,
    "css": "less"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write",
      "eslint --fix"
    ]
  },
  "scripts": {
    "prepare": "husky install",
    "mock": "mock-server -p 8001",
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "test": "jest",
    "ci": "jest --bail --silent",
    "update:deps": "yarn upgrade-interactive --latest"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
  "dependencies": {
    "@antv/f2": "^3.8.10-beta.1",
    "@babel/runtime": "^7.23.2",
    "@taro-hooks/plugin-react": "^2.0.7",
    "@taroify/core": "^0.1.1-alpha.4",
    "@tarojs/cli": "3.6.18",
    "@tarojs/components": "3.6.18",
    "@tarojs/extend": "^3.6.18",
    "@tarojs/plugin-framework-react": "3.6.18",
    "@tarojs/plugin-html": "^3.6.18",
    "@tarojs/react": "3.6.18",
    "@tarojs/runtime": "3.6.18",
    "@tarojs/taro": "3.6.18",
    "dva-core": "^2.0.4",
    "dva-immer": "^1.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "redux": "^4.2.1",
    "taro-hooks": "2.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "@liangskyli/mock": "^3.0.1",
    "@tarojs/mini-runner": "3.6.18",
    "@tarojs/webpack-runner": "3.6.18",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.7",
    "@types/react": "^18.2.34",
    "@types/webpack-env": "^1.18.3",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "babel-plugin-import": "^1.13.8",
    "babel-preset-taro": "3.6.18",
    "eslint": "^8.52.0",
    "eslint-config-taro": "3.6.18",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "stylelint": "^14.5.1",
    "stylelint-config-css-modules": "^3.0.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
